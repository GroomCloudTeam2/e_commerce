  "components": {
    "schemas": {
      "UserRole": {
        "type": "string",
        "enum": [
          "USER",
          "OWNER",
          "MANAGER",
          "MASTER"
        ]
      },
      "UserStatus": {
        "type": "string",
        "enum": [
          "ACTIVE",
          "BANNED",
          "WITHDRAWN"
        ]
      },
      "ProductStatus": {
        "type": "string",
        "enum": [
          "ON_SALE",
          "SOLD_OUT",
          "HIDDEN",
          "SUSPENDED"
        ]
      },
      "OrderStatus": {
        "type": "string",
        "enum": [
          "PENDING",
          "PAID",
          "PREPARING",
          "SHIPPING",
          "DELIVERED",
          "CONFIRMED",
          "CANCELLED"
        ]
      },
      "SignupRequest": {
        "required": [
          "email",
          "nickname",
          "password",
          "phoneNumber",
          "role"
        ],
        "type": "object",
        "properties": {
          "email": {
            "type": "string",
            "format": "email"
          },
          "password": {
            "type": "string",
            "format": "password"
          },
          "nickname": {
            "type": "string"
          },
          "phoneNumber": {
            "type": "string"
          },
          "role": {
            "$ref": "#/components/schemas/UserRole"
          },
          "store": {
            "type": "string"
          },
          "zipCode": {
            "type": "string"
          },
          "address": {
            "type": "string"
          },
          "detailAddress": {
            "type": "string"
          },
          "account": {
            "type": "string"
          },
          "bank": {
            "type": "string"
          }
        }
      },
      "LoginRequest": {
        "required": [
          "email",
          "password"
        ],
        "type": "object",
        "properties": {
          "email": {
            "type": "string"
          },
          "password": {
            "type": "string"
          }
        }
      },
      "AddressRequest": {
        "required": [
          "address",
          "detailAddress",
